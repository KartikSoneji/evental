{
	"useTabs": true,
	"printWidth": 100,
	"singleQuote": true,
	"trailingComma": "none",
	"semi": true,
	"quoteProps": "consistent",
	"endOfLine": "lf",
	"bracketSpacing": true,
	"arrowParens": "always",
	"importOrder": ["^[./]"],
	"importOrderSeparation": true,
	"importOrderSortSpecifiers": true,
	"plugins": ["@trivago/prettier-plugin-sort-imports"]
}
