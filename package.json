{
	"name": "next-template",
	"private": true,
	"scripts": {
		"prepare": "prisma generate",
		"docker": "docker-compose up -d",
		"quickstart": "yarn && yarn docker && yarn migrate:dev && yarn dev",
		"build": "prisma generate && prisma migrate deploy && next build",
		"start": "next start",
		"dev": "next dev -p 5555",
		"lint": "tsc && next lint --fix",
		"format": "tsc && prettier --write ./{server,pages,utils,components}/**/*.{js,jsx,json,md,ts,tsx} && prisma format",
		"migrate:dev": "prisma migrate dev",
		"migrate:reset": "prisma migrate reset",
		"migrate:prod": "prisma migrate deploy",
		"email:forgotPassword": "ts-node --project tsconfig.json  --compiler-options {\\\"module\\\":\\\"CommonJS\\\"} email/forgotPassword.ts"
	},
	"dependencies": {
		"@fortawesome/fontawesome-svg-core": "^6.1.1",
		"@fortawesome/free-brands-svg-icons": "^6.1.1",
		"@fortawesome/free-solid-svg-icons": "^6.1.1",
		"@fortawesome/react-fontawesome": "^0.1.18",
		"@hookform/resolvers": "^2.8.8",
		"@prisma/client": "^3.13.0",
		"@radix-ui/react-dropdown-menu": "^0.1.6",
		"@tailwindcss/forms": "^0.5.0",
		"@types/cookie": "^0.5.1",
		"@types/multiparty": "^0.0.33",
		"@upstash/redis": "^1.3.1",
		"argon2": "^0.28.5",
		"aws-sdk": "^2.1121.0",
		"axios": "^0.26.1",
		"babel-plugin-superjson-next": "^0.4.3",
		"busboy": "^1.6.0",
		"classnames": "^2.3.1",
		"cookie": "^0.5.0",
		"date-fns": "^2.28.0",
		"is-iso-date": "^0.0.1",
		"mjml": "^4.12.0",
		"multiparty": "^4.2.3",
		"next": "^12.1.5",
		"nextkit": "^3.2.4",
		"nprogress": "^0.2.0",
		"parse-multipart-data": "^1.3.0",
		"react": "17.0.2",
		"react-datepicker": "^4.7.0",
		"react-dom": "17.0.2",
		"react-hook-form": "^7.30.0",
		"react-query": "^3.35.0",
		"react-toastify": "^8.2.0",
		"sharp": "^0.30.4",
		"superjson": "^1.9.0",
		"zod": "^3.14.4"
	},
	"devDependencies": {
		"@types/busboy": "^1.5.0",
		"@types/mjml": "^4.7.0",
		"@types/node": "16.11.7",
		"@types/nprogress": "^0.2.0",
		"@types/react": "^18.0.8",
		"@types/react-datepicker": "^4.4.1",
		"@types/sharp": "^0.30.2",
		"autoprefixer": "^10.4.0",
		"eslint": "^7.32.0",
		"eslint-config-next": "^12.0.4",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-jsx-a11y": "^6.5.1",
		"eslint-plugin-prettier": "^4.0.0",
		"eslint-plugin-react": "^7.27.1",
		"eslint-plugin-react-app": "^6.2.2",
		"eslint-plugin-react-hooks": "^4.3.0",
		"postcss": "^8.3.11",
		"prettier": "^2.5.0",
		"prisma": "3.13.0",
		"tailwindcss": "^2.2.19",
		"ts-node": "^10.5.0",
		"typescript": "^4.6.3"
	},
	"lint-staged": {
		"*.{js,jsx,ts,tsx}": [
			"eslint --fix",
			"npx prettier --write"
		],
		"*.{json,md,yaml,yml,scss,css}": [
			"npx prettier --write"
		],
		"*.js": "eslint --cache --fix"
	}
}
